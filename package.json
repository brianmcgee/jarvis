{
  "name": "jarvis",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint '*/**/*.{js,ts,tsx,yml,yaml,json}'",
    "lint:fix": "eslint '*/**/*.{js,ts,tsx,yml,yaml}' --quiet --fix",
    "dev": "graphql-let && next dev",
    "build": "graphql-let && next build",
    "start": "next start",
    "sync": "ts-node -r tsconfig-paths/register lib/sync",
    "sync:cron": "ts-node -r tsconfig-paths/register lib/sync/cron",
    "prisma:studio": "prisma studio",
    "prisma:migrate": "prisma migrate dev --preview-feature",
    "storybook": "start-storybook -s ./public -p 9000",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@apollo/client": "^3.3.10",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@graphql-tools/load-files": "^6.2.5",
    "@graphql-tools/merge": "^6.2.7",
    "@graphql-tools/schema": "^7.1.3",
    "@prisma/client": "2.16.1",
    "@types/asana": "^0.18.1",
    "apollo-server-micro": "^2.21.0",
    "apollo-type-bigint": "^0.1.3",
    "asana": "^0.18.5",
    "autoprefixer": "^10.2.4",
    "classnames": "^2.2.6",
    "clubhouse-lib": "^0.10.0",
    "dayjs": "^1.10.4",
    "graphql": "^15.5.0",
    "next": "10.0.6",
    "node-cron": "^2.0.3",
    "pino": "^6.11.1",
    "pino-prettifier": "^1.0.8",
    "pino-pretty": "^4.5.0",
    "postcss": "^8.2.4",
    "postcss-font-magician": "^3.0.0",
    "postcss-import": "^14.0.0",
    "postcss-nested": "^5.0.3",
    "postcss-preset-env": "^6.7.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@graphql-codegen/cli": "^1.20.1",
    "@graphql-codegen/plugin-helpers": "^1.18.2",
    "@graphql-codegen/typescript": "^1.21.0",
    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "@graphql-codegen/typescript-resolvers": "^1.18.2",
    "@storybook/addon-essentials": "^6.1.18",
    "@storybook/addon-links": "^6.1.18",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.1.18",
    "@types/node": "^14.14.25",
    "@types/pino": "^6.3.5",
    "@types/react": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-yaml": "^0.3.0",
    "graphql-let": "^0.16.3",
    "graphql-tag": "^2.11.0",
    "husky": "^5.0.9",
    "js-yaml-loader": "^1.2.2",
    "postcss-custom-properties": "^11.0.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "prettier": "^2.2.1",
    "prisma": "2.17.0",
    "storybook-css-modules-preset": "^1.0.6",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3",
    "yaml-loader": "^0.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
